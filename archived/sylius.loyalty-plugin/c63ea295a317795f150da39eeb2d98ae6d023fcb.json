{
    "manifests": {
        "sylius/loyalty-plugin": {
            "manifest": {
                "bundles": {
                    "Sylius\\LoyaltyPlugin\\SyliusLoyaltyPlugin": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "add-lines": [
                    {
                        "file": "assets/admin/entrypoint.js",
                        "content": "import '../../vendor/sylius/loyalty-plugin/assets/admin/entrypoint';",
                        "position": "bottom",
                        "warn_if_missing": true
                    },
                    {
                        "file": "assets/shop/entrypoint.js",
                        "content": "import '../../vendor/sylius/loyalty-plugin/assets/shop/entrypoint';",
                        "position": "bottom",
                        "warn_if_missing": true
                    }
                ]
            },
            "files": {
                "config/packages/sylius_loyalty_plugin.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->import('@SyliusLoyaltyPlugin/config/config.yaml');",
                        "};",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/sylius_loyalty_plugin_admin.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\Routing\\Loader\\Configurator\\RoutingConfigurator;",
                        "",
                        "return static function (RoutingConfigurator $routingConfigurator): void {",
                        "    $routingConfigurator->import('@SyliusLoyaltyPlugin/config/admin_routing.yaml')",
                        "        ->prefix('/%sylius_admin.path_name%');",
                        "};",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/sylius_loyalty_plugin_shop.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\Routing\\Loader\\Configurator\\RoutingConfigurator;",
                        "",
                        "return static function (RoutingConfigurator $routingConfigurator): void {",
                        "    $routingConfigurator->import('@SyliusLoyaltyPlugin/config/shop_routing.yaml')",
                        "        ->prefix('/{_locale}')",
                        "        ->requirements([",
                        "            '_locale' => '^[a-z]{2}(?:_[A-Z]{2})?$',",
                        "        ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "c63ea295a317795f150da39eeb2d98ae6d023fcb"
        }
    }
}
