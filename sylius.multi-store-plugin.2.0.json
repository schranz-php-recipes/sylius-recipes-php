{
    "manifests": {
        "sylius/multi-store-plugin": {
            "manifest": {
                "bundles": {
                    "Sylius\\MultiStorePlugin\\SyliusMultiStorePlugin": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "add-lines": [
                    {
                        "file": "assets/admin/entrypoint.js",
                        "content": "import '../../vendor/sylius/multi-store-plugin/assets/admin/entrypoint';",
                        "position": "bottom",
                        "warn_if_missing": true
                    }
                ]
            },
            "files": {
                "config/packages/sylius_multi_store_plugin.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->import('@SyliusMultiStorePlugin/config/config.yaml');",
                        "};",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/sylius_multi_store_plugin.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\Routing\\Loader\\Configurator\\RoutingConfigurator;",
                        "",
                        "return static function (RoutingConfigurator $routingConfigurator): void {",
                        "    $routingConfigurator->import('@SyliusMultiStorePlugin/config/admin_routing.yaml')",
                        "        ->prefix('/%sylius_admin.path_name%');",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "4c872e66d4a268a4542e86689a458c757ea0dab8"
        }
    }
}
